{
	"extends": ["../../.eslintrc.json"],
	"ignorePatterns": ["!**/*"],
	"overrides": [
		{
			"files": ["*.ts"],
			"extends": [
				"plugin:@nx/angular",
				"plugin:@angular-eslint/template/process-inline-templates"
			],
			"rules": {
				"@angular-eslint/directive-selector": "off",
				"@angular-eslint/component-selector": "off"
			}
		},
		{
			"files": ["*.html"],
			"extends": ["plugin:@nx/angular-template"],
			"rules": {}
		}
	]
}
